issn = {1751-8687},
journal = {IET Generation, Transmission {\{}{\&}{\}} Distribution},
keywords = {,MMC,Simulink-Matlab,feedforward,feedforward control,grounded neutral,harmonic analysis,harmonic function analysis,harmonic voltage components,low-frequency harmonic reduction,low-frequency harmonic voltage,mathematics computing,modular multilevel converter,open loop control,power convertors,power system control,power system harmonics,power transformers,signal modulation,single-phase AC power,transformers,voltage control},
