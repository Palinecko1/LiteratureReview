keywords = {,Harmonic distortion,hybrid PWM,interleaving,pulse width modulation (PWM),space vector,switching sequences,three-phase inverters},
number = {7},
pages = {1954--1967},
title = {{Hybrid interleaved space vector PWM for ripple reduction in modular converters}},
volume = {26},
year = {2011}
}
