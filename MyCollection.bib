journal = {IEEE Transactions on Power Electronics},
keywords = {,DC-DC power convertors,bidirectional triple-voltage DC-DC converter,bridge circuits,fuel cell power plants,fuel cell vehicle power systems,fuel cell vehicles,half-bridge circuit,high-frequency transformer,high-frequency transformers,hybrid electric vehicles,hybrid power systems,hybrid vehicle power systems,interleaved multiphase modular configuration,power 4.5 kW,soft switching,switching convertors,traction drive,traction motor drives,transformer leakage inductance,voltage 14 V,voltage 42 V},
number = {6},
pages = {3035--3046},
title = {{A multiphase , modular , bidirectional , triple-Voltage DC-DC converter for hybrid and fuel cell vehicle power systems}},
