journal = {IEEE Transactions on Power Delivery},
keywords = {,Automatic voltage control,Converters,Current balancing,Energy storage,HVDC Transmission,HVDC transmission,Harmonic analysis,Inductors,STATCOM,Switches,current source modular multilevel converter},
number = {99},
pages = {1},
title = {{Current Source Modular Multilevel Converter: Detailed Analysis and STATCOM Application}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=7180392},
volume = {PP},
year = {2015}
}
