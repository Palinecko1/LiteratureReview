journal = {IEEE Transactions on Power Electronics},
keywords = {,DC-DC converter,input-series-output-series (ISOS),input-voltage sharing (IVS),output-voltage sharing (OVS)},
number = {7},
pages = {3766--3775},
title = {{A general control strategy for input-series-output-series modular DC-DC converters}},
volume = {29},
year = {2014}
}
