keywords = {,1-D methods,Maxwell's equations,analytical approach,current distribution,impedances,lumped circuit model,planar magnetics},
number = {1},
pages = {560--580},
title = {{A Systematic Approach to Modeling Impedances and Current Distribution in Planar Magnetics}},
volume = {31},
year = {2016}
}
