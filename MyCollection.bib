year = {2012}
}
@article{Far2016,
abstract = {This article presents three dynamic linear state space models of Modular Multilevel Converter (MMC) which are suitable for small-signal dynamic studies and controller design. The three models differ by the number of states (two, six and ten) and therefore are suitable for different applications based on the required accuracy. The 2nd and 6th order models ignore dynamics of the second harmonics and circulating current suppression control (CCSC). The main challenges of dynamic analytical modelling of MMC are the non-linear multiplication terms in ABC frame equations for modulating oscillating signals. The multiplication non-linear terms are therefore considered directly in the rotating DQ frame. This requires simultaneous modeling in zero sequence, fundamental frequency DQ and double fundamental frequency DQ2 frames. The proposed linear analytical models are implemented in state-space in MATLAB. The validity and accuracy of the models are verified against detailed 401 level MMC model in PSCAD/EMTDC in both: time and frequency domains. The results show very good accuracy for the 10th order model and decreasing accuracy for the lower order models. The influence of PLL and CCSC gains on MMC dynamic is also studied using the proposed 10th model and the results verify the suitability of the model for this purpose.},
author = {Far, Aliakbar Jamshidi and Jovcic, Dragan},
doi = {10.1109/TPWRD.2015.2478489},
file = {:H$\backslash$:/My Documents/Papers/Literature Review - modular and converter/publication tittle/201-225/07279188.pdf:pdf},
isbn = {9781509041688},
issn = {08858977},
journal = {IEEE Transactions on Power Delivery},
keywords = {,Converters,HVDC transmission,power system control,power system dynamics,power system modeling},
number = {1},
pages = {191--199},
title = {{Small-Signal Dynamic DQ Model of Modular Multilevel Converter for System Studies}},
volume = {31},
year = {2016}
}
@article{Fazel2007,
abstract = {The development of new high power semiconductors such as 3.3kV, 4.5kV,{\$}\backslash{\$}nand 6.5kV Insulated{\$}\backslash{\$}n{\$}\backslash{\$}nGate Bipolar Transistors (IGBTs), and 4.5kV to 5.5kV Integrated Gate{\$}\backslash{\$}nCommutated Thyristors{\$}\backslash{\$}n{\$}\backslash{\$}n(IGCTs), and improved converter designs have led to a drastic increase{\$}\backslash{\$}nof the market share of{\$}\backslash{\$}n{\$}\backslash{\$}nPulse-Width-Modulated (PWM) controlled Voltage Source Converters (VSC){\$}\backslash{\$}n[21]. Despite a{\$}\backslash{\$}n{\$}\backslash{\$}nprice reduction of Gate Turn-Off Thyristors (GTOs) by a factor of{\$}\backslash{\$}ntwo to three over the last five{\$}\backslash{\$}n{\$}\backslash{\$}nyears, also conventional GTO Voltage Source Converters and Current{\$}\backslash{\$}nSource Converters (CSC){\$}\backslash{\$}n{\$}\backslash{\$}nare increasingly replaced by PWM Voltage Source Converters with IGCTs{\$}\backslash{\$}nor IGBTs in traction{\$}\backslash{\$}n{\$}\backslash{\$}nand industry applications [21].{\$}\backslash{\$}n{\$}\backslash{\$}n{\$}\backslash{\$}nToday the two-level Voltage Source Converter (2L-VSC) applying IGBTs{\$}\backslash{\$}nis clearly the{\$}\backslash{\$}n{\$}\backslash{\$}ndominating converter topology in traction applications (low, medium,{\$}\backslash{\$}nand high power{\$}\backslash{\$}n{\$}\backslash{\$}npropulsion) and the three-level Neutral Point Clamped Voltage Source{\$}\backslash{\$}nConverter (3L-NPC{\$}\backslash{\$}n{\$}\backslash{\$}nVSC) is mostly applied in industrial medium voltage converters. The{\$}\backslash{\$}n2L-VSC and the 3L-NPC{\$}\backslash{\$}n{\$}\backslash{\$}nVSC offer technical advantages like a simple power part, a low component{\$}\backslash{\$}ncount, and{\$}\backslash{\$}n{\$}\backslash{\$}nstraightforward protection and modulation schemes. On the other hand,{\$}\backslash{\$}nthe hard-switching{\$}\backslash{\$}n{\$}\backslash{\$}ntransients of the power semiconductors at high commutation voltage{\$}\backslash{\$}ncause high switching losses{\$}\backslash{\$}n{\$}\backslash{\$}nand a poor harmonic spectrum which produces additional losses in the{\$}\backslash{\$}nmachine. Further{\$}\backslash{\$}n{\$}\backslash{\$}nproblems are created by over-voltages in cables and machines as well{\$}\backslash{\$}nas bearing currents due to{\$}\backslash{\$}n{\$}\backslash{\$}nthe steep-switching transients [5], [21], [121].{\$}\backslash{\$}n{\$}\backslash{\$}n{\$}\backslash{\$}nMulti-level converters (MLCs) have been receiving attention in the{\$}\backslash{\$}nrecent years and have been{\$}\backslash{\$}n{\$}\backslash{\$}nproposed as the best choice in a wide variety of medium voltage (MV){\$}\backslash{\$}napplications [47]. They{\$}\backslash{\$}n{\$}\backslash{\$}nenable a commutation at substantially reduced voltages and an improved{\$}\backslash{\$}nharmonic spectrum{\$}\backslash{\$}n{\$}\backslash{\$}nwithout a series connection of devices, which is the main advantage{\$}\backslash{\$}nof a multi-level structure.{\$}\backslash{\$}n{\$}\backslash{\$}nOther advantages of these topologies are better output voltage quality,{\$}\backslash{\$}nreduced electromagnetic{\$}\backslash{\$}n{\$}\backslash{\$}ninterference (EMI) problems, and lower overall losses in some cases.{\$}\backslash{\$}nHowever, today they have{\$}\backslash{\$}n{\$}\backslash{\$}na limited commercial impact due to their disadvantages such as high{\$}\backslash{\$}ncontrol complexity and{\$}\backslash{\$}n{\$}\backslash{\$}nincreased power semiconductor count compared to the 2L-VSC and the{\$}\backslash{\$}n3L-NPC VSC.{\$}\backslash{\$}n{\$}\backslash{\$}nThere is a large variety of power semiconductors (e.g. IGBTs, GTOs,{\$}\backslash{\$}nIGCTs) and converter{\$}\backslash{\$}n{\$}\backslash{\$}ntopologies (e.g. 2L-VSC, 3L-NPC VSC, 3L-FLC VSC, 4L-FLC VSC, and SCHB{\$}\backslash{\$}nVSCs).{\$}\backslash{\$}n{\$}\backslash{\$}nHowever, today there is no comparative analysis of the different converter{\$}\backslash{\$}ntopologies.{\$}\backslash{\$}n{\$}\backslash{\$}nTherefore, the objective of this thesis is a detailed comparison of{\$}\backslash{\$}nstate-of-the-art 2L-VSC, 3LNPC{\$}\backslash{\$}n{\$}\backslash{\$}nVSC, and different multi-level VSCs (e.g. 3L-FLC VSC, 4L-FLC VSC,{\$}\backslash{\$}n5L-, 7L-, and 9LSC2LHB{\$}\backslash{\$}n{\$}\backslash{\$}nVSCs) for medium voltage converters. On the basis of the application{\$}\backslash{\$}nrequirements,{\$}\backslash{\$}n{\$}\backslash{\$}ndifferent ML converter structures are designed, simulated, and evaluated.{\$}\backslash{\$}nThe development of{\$}\backslash{\$}n{\$}\backslash{\$}ndesign tools based on state-of-the-art converters and semiconductors,{\$}\backslash{\$}nwhich enable the{\$}\backslash{\$}n{\$}\backslash{\$}ndimensioning of power semiconductors, dc link capacitors, and transformers;{\$}\backslash{\$}nare one major part{\$}\backslash{\$}n{\$}\backslash{\$}nof this thesis. Finally, the amount of active and passive components,{\$}\backslash{\$}nthe modulation, losses, and{\$}\backslash{\$}n{\$}\backslash{\$}nefficiency of aforementioned converters are calculated and compared.{\$}\backslash{\$}n{\$}\backslash{\$}nThe thesis is arranged in seven main chapters: This introduction is{\$}\backslash{\$}nfollowed by an overview of{\$}\backslash{\$}n{\$}\backslash{\$}nmedium voltage converter topologies, including medium voltage power{\$}\backslash{\$}nsemiconductors and{\$}\backslash{\$}n{\$}\backslash{\$}nmodulations in chapter 2.{\$}\backslash{\$}n{\$}\backslash{\$}n2 INTRODUCTION{\$}\backslash{\$}n{\$}\backslash{\$}nChapter 3 presents the basic structure and function of voltage source{\$}\backslash{\$}nconverter topologies.{\$}\backslash{\$}n{\$}\backslash{\$}nBased on the requirements for MV applications, advantages and disadvantages{\$}\backslash{\$}nof the{\$}\backslash{\$}n{\$}\backslash{\$}ntopologies are discussed.{\$}\backslash{\$}n{\$}\backslash{\$}nOne of the main parts of this thesis is the modelling and simulation{\$}\backslash{\$}nof the different multi-level{\$}\backslash{\$}n{\$}\backslash{\$}nconverters. The dimensioning and design of power semiconductors, dc{\$}\backslash{\$}nlink capacitors, and{\$}\backslash{\$}n{\$}\backslash{\$}nisolation transformers are developed in chapter 4.{\$}\backslash{\$}n{\$}\backslash{\$}nThe basic converter data for a converter comparison, including the{\$}\backslash{\$}nIGBTs, modulation,{\$}\backslash{\$}n{\$}\backslash{\$}nswitching frequency, and state of the art are described in chapter{\$}\backslash{\$}n5.{\$}\backslash{\$}n{\$}\backslash{\$}nThe comparison of the different converter topologies is performed{\$}\backslash{\$}nin chapter 6 for 3L-NPC{\$}\backslash{\$}n{\$}\backslash{\$}nVSC, 3L-FLC VSC, 4L-FLC VSC, and 5L-, 7L-, 9L-, and 11L-SCHB VSCs{\$}\backslash{\$}non the basis of the{\$}\backslash{\$}n{\$}\backslash{\$}nstate-of-the-art 1.7kV, 2.5kV, 3.3kV, 4.5kV, and 6.5kV IGBTs for 2.3kV,{\$}\backslash{\$}n3.3kV, 4.16kV, and{\$}\backslash{\$}n{\$}\backslash{\$}n6kV medium voltage converters. As a result, the converter losses,{\$}\backslash{\$}nthe semiconductor loss{\$}\backslash{\$}n{\$}\backslash{\$}ndistribution, the converter efficiency, harmonic spectrum analysis,{\$}\backslash{\$}nand the installed switch{\$}\backslash{\$}n{\$}\backslash{\$}npower of the different converter topologies are compared in this chapter.{\$}\backslash{\$}n{\$}\backslash{\$}nFinally, the conclusion and discussion are presented in chapter 7.},
author = {Fazel, Seyed Saeed},
file = {:H$\backslash$:/My Documents/Papers/phd/Multilevel Converter/Berlin-dissertation-comparison MMC.pdf:pdf},
journal = {von der Fakult{\{}{\"{a}}{\}}t IV- Elektrotechnik und Informatik der Technischen Universit{\{}{\"{a}}{\}}t Berlin, Institut f{\{}{\"{u}}{\}}r Energie- und Automatisierungstechnik},
title = {{Investigation and comparison of multi-level converters for medium voltage applications}},
year = {2007}
}
