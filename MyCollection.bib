keywords = {,DC-DC converter and fuel cell,hybrid,inductor,interleaving,planar-integrated magnetics (PIM),transformer},
number = {11},
pages = {3254--3264},
title = {{Planar-integrated magnetics (PIM) module in hybrid bidirectional DC-DC converter for fuel cell application}},
volume = {26},
year = {2011}
}
