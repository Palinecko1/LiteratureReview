@article{Ajaei2015a,
abstract = {The DC cables of the Voltage-Sourced Converter-based HVDC link, including the Modular Multilevel Converter (MMC) configuration, must be protected by surge arresters against transient overvoltages. Selection of the appropriate surge arresters requires a comprehensive understanding of the phenomena that cause the overvoltages and accurate calculation of energy discharged in the arresters. This paper investigates the mechanism that causes transient overvoltages, due to DC-side line-to-ground faults, in a cable-connected MMC-HVDC link. The paper also evaluates the impacts of various system parameters, e.g., the pre-fault power transfer level, the MMC arm inductance, the transformer leakage inductance, the fault location, the AC system short-circuit capacity, the cable inductance, and the MMC blocking delay, on the peak transient overvoltages and the amount of energy discharge in the cable surge arresters. The transient overvoltage studies are also conducted when the DC cable is replaced by an over-head line. The studies are performed in the PSCAD platform. The studies conclude that the pre-fault power transfer level and the MMC blocking delay have the most significant impacts on the surge arrester energy discharge under DC-side line-to-ground faults in the MMC-HVDC link. The study results also provide essential information for the design of the MMC-HVDC link components.},
author = {Ajaei, Firouz and Iravani, Reza},
doi = {10.1109/TPWRD.2015.2477493},
file = {:H$\backslash$:/My Documents/Papers/Literature Review - modular and converter/key words/101-125/07279160.pdf:pdf},
issn = {0885-8977},
journal = {IEEE Transactions on Power Delivery},
number = {c},
pages = {1},
title = {{Cable Surge Arrester Operation Due to Transient Overvoltages Under DC-Side Faults in the MMC-HVDC Link}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=7279160},
volume = {8977},
year = {2015}
}
