file = {:H$\backslash$:/My Documents/Papers/23062016/06484142.pdf:pdf;:H$\backslash$:/My Documents/Papers/Literature Review - modular and converter/publication tittle/1-25/06484142.pdf:pdf},
isbn = {9789075815177},
