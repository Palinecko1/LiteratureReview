number = {99},
pages = {1},
title = {{Efficient Modeling of an MMC-Based Multiterminal DC System Employing Hybrid HVDC Breakers}},
url = {http://ieeexplore.ieee.org/ielx7/61/7163645/07029709.pdf?tp={\{}{\&}{\}}arnumber=7029709{\{}{\&}{\}}isnumber=7163645 http://ieeexplore.ieee.org/ielx7/61/7163645/07029709.pdf?tp={\&}arnumber=7029709{\&}isnumber=7163645},
