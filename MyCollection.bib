journal = {Renewable and Sustainable Energy Reviews},
keywords = {,Distributed energy,Integrated power electronics modules,Modularity,Power electronics interface},
number = {9},
pages = {2323--2335},
title = {{A review of power electronics interfaces for distributed energy systems towards achieving low-cost modular design}},
volume = {13},
year = {2009}
}
