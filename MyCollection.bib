journal = {IEEE Transactions on Power Electronics},
keywords = {,Capacitors,Control systems,Educational institutions,Frequency control,Harmonic analysis,Harmonics suppression,Modular multilevel converter (MMC),Voltage control,circulating current,harmonics suppression,repetitive control},
