keywords = {,Input-series and output-parallel (ISOP),Input-series connection,Load sharing,Modular converter,Voltage sharing},
number = {6},
pages = {1462--1473},
title = {{Active input-voltage and load-current sharing in input-series and output-parallel connected modular dc-dc converters using dynamic input-voltage reference scheme}},
volume = {19},
year = {2004}
}
