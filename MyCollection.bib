keywords = {,Common duty ratio,Fault tolerance,Input series and output parallel (ISOP),Input-series connection,Input-voltage sharing,Load-current sharing,Modular dc-dc converters},
number = {4},
pages = {1101--1111},
title = {{Common-duty-ratio control of input-series connected modular DC-DC converters with active input voltage and load-current sharing}},
volume = {42},
