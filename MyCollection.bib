@article{Son2012,
abstract = {This paper presents design and control methods for fault-tolerant operations with redundant converter modules, one of the most prominent features in modular multilevel converter (MMC) topology. In fully implementing MMC functionalities, a nearest level control is applied as a low-switching modulation method. A dual sorting algorithm is newly proposed for effectively reducing the switching commutations of each power module as well as for voltage balancing control. Built upon these primary MMC topological and control features, its redundant operation is comprehensively investigated for fail-safe energy transfer. In particular, a novel spare process is proposed to handle an emergency situation when the number of faulty power modules exceeds the module redundancy. Since topological redundancy may cause the switching commutations of power modules in an arm to be unevenly distributed, a practical and effective mitigation measure is incorporated to keep the energy balance while avoiding the undesired switching stresses. Rigorous simulation studies for MMC and its application for high-voltage direct current are performed to demonstrate the validity and effectiveness of the proposed spare process under normal and emergency conditions.},
author = {Son, Gum Tae and Lee, Hee Jin and Nam, Tae Sik and Chung, Yong Ho and Lee, Uk Hwa and Baek, Seung Taek and Hur, Kyeon and Park, Jung Wook},
doi = {10.1109/TPWRD.2012.2190530},
file = {:H$\backslash$:/My Documents/Papers/Literature Review - modular and converter/publication tittle/276-300/06185713.pdf:pdf},
isbn = {0885-8977},
issn = {08858977},
journal = {IEEE Transactions on Power Delivery},
keywords = {,Dual sorting algorithm,modular multilevel converter,nearest level control,redundancy process,spare process,voltage-source converter-high-voltage direct curre},
number = {3},
pages = {1611--1619},
title = {{Design and control of a modular multilevel HVDC converter with redundant power modules for noninterruptible energy transfer}},
volume = {27},
year = {2012}
}
@article{Song2013,
abstract = {Modular multilevel converters (MMC) are considered a top converter alternative for voltage-source converter (VSC) high-voltage, direct current (HVDC) applications. Main circuit design and converter performance evaluation are always important issues to consider before installing a VSC-HVDC system. Investigation into a steady-state analysis method for an MMC-based VSC-HVDC system is necessary. This paper finds a circular interaction among the electrical quantities in an MMC. Through this circular interaction, a key equation can be established to solve the unknown circulating current. A new steady-state model is developed to simply and accurately describe the explicit analytical expressions for various voltage and current quantities in an MMC. The accuracy of the expressions is improved by the consideration of the circulating current when deriving all the analytical expressions. The model's simplicity is demonstrated by having only one key equation to solve. Based on the analytical expressions for the arm voltages, the equivalent circuits for MMC are proposed to improve the current understanding of the operation of MMC. The feasibility and accuracy of the proposed method are verified by comparing its results with the simulation and experimental results.},
author = {Song, Qiang and Liu, Wenhua and Li, Xiaoqian and Rao, Hong and Xu, Shukai and Li, Licheng},
doi = {10.1109/TPEL.2012.2227818},
file = {:H$\backslash$:/My Documents/Papers/Literature Review - modular and converter/publication tittle/176-200/06353957.pdf:pdf},
isbn = {0885-8993 VO - 28},
issn = {08858993},
journal = {IEEE Transactions on Power Electronics},
keywords = {,Modular multilevel converter (MMC),Steady-state analysis,VSC-HVDC,Voltage-source converter HVDC},
number = {8},
pages = {3702--3713},
title = {{A steady-state analysis method for a modular multilevel converter}},
volume = {28},
year = {2013}
}
