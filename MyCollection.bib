keywords = {,Capacitors,Pulse width modulation,Pulse-width modulation,Redundancy,Sorting,Switches,Switching frequency,modular multilevel converter,multilevel converters},
number = {c},
pages = {1},
title = {{Switching Frequency Analysis of Staircase Modulated Modular Multilevel Converters and Equivalent PWM Techniques}},
volume = {PP},
year = {2015}
}
